docker run --rm -it -p 0.0.0.0:80:80 -p 0.0.0.0:443:443 --network="host" -e ASPNETCORE_URLS="https://0.0.0.0:443;http://0.0.0.0:80"  -e ASPNETCORE_HTTPS_PORT=443 -e ASPNETCORE_Kestrel__Certificates__Default__Password="analysim" -e ASPNETCORE_Kestrel__Certificates__Default__Path=/https/fullchain.pfx -v ~/.aspnet/https:/https/   analysim-dev 
